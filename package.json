{
  "name": "gessa-component-library",
  "version": "0.0.8",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "bump-version:major": "npm version major --force && cd libs/dsl && npm version major --force",
    "bump-version:minor": "npm version minor --force && cd libs/dsl && npm version minor --force",
    "bump-version:patch": "npm version patch --force && cd libs/dsl && npm version patch --force",
    "build": "nx build",
    "test": "nx test",
    "container:build:prod": "nx build container --prod -- --memoryLimit=8192",
    "container:build:prod:stats": "nx build container --prod --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "container:build:dev:stats": "nx build container --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "analyze:prod:stats": "run-s \"container:build:prod:stats\" \"webpack-bundle-analyzer\"",
    "analyze:dev:stats": "run-s \"container:build:dev:stats\" \"webpack-bundle-analyzer\"",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/apps/container/stats.json",
    "dsl:build:prod": "nx build dsl --prod -- --memoryLimit=8192 ",
    "dsl:build:prod:stats": "nx build dsl --prod --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "dsl:build:dev:stats": "nx build dsl --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "bundle:report": "source-map-explorer dist/apps/container**/*.js"
  },
  "private": true,
